<window id="115">
	<defaultcontrol>1</defaultcontrol>
	<include>Fade_Dialog_OpenClose_Animation</include>
	<visible>Player.Seeking | Player.DisplayAfterSeek | [Player.ShowInfo + Window.IsActive(FullScreenVideo)] | Window.IsActive(VideoOSD) | Window.IsActive(MusicOSD) | Player.Paused | Player.Forwarding | Player.Rewinding</visible>
	<controls>
		<!-- playstate images -->
		<control type="image">
			<description>pause image</description>
			<align>right</align>
			<posx>530</posx>
			<posy>100</posy>
			<height>50</height>
			<width>210</width>
			<visible>Player.Paused | Player.Forwarding | Player.Rewinding</visible>
			<texture>OSD-SeekBar-Paused.png</texture>
		</control>
		<control type="image">
			<description>FF image</description>
			<align>right</align>
			<posx>565</posx>
			<posy>110</posy>
			<height>19</height>
			<width>20</width>
			<visible>Player.Forwarding</visible>
			<texture>FF.png</texture>
		</control>
		<control type="image">
			<description>RW image</description>
			<align>right</align>
			<posx>565</posx>
			<posy>110</posy>
			<height>19</height>
			<width>20</width>
			<visible>Player.Rewinding</visible>
			<texture>RW.png</texture>
		</control>
		<control type="label">
			<description>Paused Label</description>
			<align>center</align>
			<posx>540</posx>
			<posy>108</posy>
			<width>110</width>
			<font>font13</font>
			<textcolor>green2</textcolor>
			<label>112</label>
			<visible>Player.Paused</visible>
		</control>
		<control type="label">
			<description>FF-RW x2 Label</description>
			<posx>590</posx>
			<posy>108</posy>
			<width>60</width>
			<font>font13</font>
			<textcolor>green2</textcolor>
			<label>x2</label>
			<visible>Player.Forwarding2x | Player.Rewinding2x</visible>
		</control>
		<control type="label">
			<description>FF-RW x4 Label</description>
			<posx>590</posx>
			<posy>108</posy>
			<width>60</width>
			<font>font13</font>
			<textcolor>green2</textcolor>
			<label>x4</label>
			<visible>Player.Forwarding4x | Player.Rewinding4x</visible>
		</control>
		<control type="label">
			<description>FF-RW x8 Label</description>
			<posx>590</posx>
			<posy>108</posy>
			<width>60</width>
			<font>font13</font>
			<textcolor>green2</textcolor>
			<label>x8</label>
			<visible>Player.Forwarding8x | Player.Rewinding8x</visible>
		</control>
		<control type="label">
			<description>FF-RW x16 Label</description>
			<posx>590</posx>
			<posy>108</posy>
			<width>60</width>
			<font>font13</font>
			<textcolor>green2</textcolor>
			<label>x16</label>
			<visible>Player.Forwarding16x | Player.Rewinding16x</visible>
		</control>
		<control type="label">
			<description>FF-RW x32 Label</description>
			<posx>590</posx>
			<posy>108</posy>
			<width>60</width>
			<font>font13</font>
			<textcolor>green2</textcolor>
			<label>x32</label>
			<visible>Player.Forwarding32x | Player.Rewinding32x</visible>
		</control>
		<!-- progressbar dialog -->
		<control type="image">
			<description>Popup Seek Dialog Background</description>
			<posx>450</posx>
			<posy>20</posy>
			<width>300</width>
			<height>90</height>
			<texture>OSD-SeekBar-Back.png</texture>
		</control>
		<control type="label">
			<description>Elapsed Time Label</description>
			<posx>550</posx>
			<posy>43</posy>
			<font>font10</font>
			<align>right</align>
			<info>Player.Time</info>
			<width>100</width>
			<height>16</height>
			<visible>!Player.Seeking</visible>
			<textcolor>green2</textcolor>
		</control>
		<control type="label">
			<description>Seek Time Label</description>
			<posx>550</posx>
			<posy>43</posy>
			<font>font10</font>
			<align>right</align>
			<info>Player.SeekTime</info>
			<width>100</width>
			<height>16</height>
			<visible>Player.Seeking</visible>
			<textcolor>green2</textcolor>
		</control>
		<control type="label">
			<description>Duration Label</description>
			<posx>555</posx>
			<posy>43</posy>
			<label>$INFO[Player.Duration,/ ]</label>
			<font>font10</font>
			<align>left</align>
			<width>100</width>
			<height>16</height>
			<textcolor>green2</textcolor>
		</control>
		<control type="progress">
			<description>Progressbar</description>
			<posx>480</posx>
			<posy>65</posy>
			<width>160</width>
			<height>3</height>
			<info>Player.Progress</info>
			<texturebg>osd_progress_back.png</texturebg>
			<midtexture>progress_mid.png</midtexture>
			<overlaytexture>-</overlaytexture>
		</control>
		<control type="slider" id="401">
			<description>Seek Slider</description>
			<posx>480</posx>
			<posy>60</posy>
			<width>160</width>
			<height>15</height>
			<texturesliderbar>osd-osd_slider_bg.png</texturesliderbar>
			<textureslidernib>osd-osd_slider_nibfo.png</textureslidernib>
			<textureslidernibfocus>osd-osd_slider_nibfo.png</textureslidernibfocus>
			<onup>1</onup>
			<ondown>1</ondown>
			<onleft>1</onleft>
			<onright>1</onright>
		</control>
	</controls>
</window>